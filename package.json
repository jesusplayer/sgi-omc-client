{
  "name": "fosa-store-client",
  "version": "21.0.8",
  "private": true,
  "scripts": {
    "4400": "npm run write:env -s && ng serve --host 0.0.0.0 --port 4400 --live-reload false --proxy-config proxy.conf.js && json-server --watch db.json && concurrently",
    "4600": "npm run write:env -s && ng serve --host 0.0.0.0 --port 4600 --proxy-config proxy.conf.js && json-server --watch db.json && concurrently",
    "4700": "npm run write:env -s && ng serve --host 0.0.0.0 --port 4700 --proxy-config proxy.conf.js && json-server --watch db.json && concurrently",
    "new:flow": "ng g @angular/core:control-flow",
    "upgrade13": "ng update @angular/core@13 @angular/cli@13 --force",
    "upgrade14": "ng update @angular/core@14 @angular/cli@14 --force",
    "upgrade15": "ng update @angular/core@15 @angular/cli@15 --force",
    "upgrade16": "ng update @angular/core@16 @angular/cli@16 --force",
    "upgrade17": "ng update @angular/core@17 @angular/cli@17 --force",
    "upgrade18": "ng update @angular/core@18 @angular/cli@18 --force",
    "up:primeng": "ng update primeng --force",
    "up:signals": "ng update @ngrx/signals --force",
    "upgrade19": "ng update @angular/core@19 @angular/cli@19 --force",
    "upgrade20": "ng update @angular/core@20 @angular/cli@20 @angular/cdk@20 --force",
    "upgrade21": "ng update @angular/cdk@20 --force && ng update @ngrx/signals --force && ng update --force",
    "installsh": "ng add @angular-eslint/schematics --force",
    "installcy": "ng add @cypress/schematic --force",
    "installpw": "ng add @playwright/test --force",
    "install:pw": "ng add playwright-ng-schematics --force",
    "update": "ng update @angular/cli @angular/core @angular/cdk --force",
    "installkc": "ng add keycloak-js --legacy-peer-deps --force",
    "ngv": "ng --version",
    "ng": "ng",
    "ngsw:generate": "npx ngsw-config dist/production/browser ngsw-config.json",
    "ngsw:generate:fb": "npx ngsw-config dist/prodboard/browser ngsw-config.json",
    "del:unused:fonts": "del-cli dist\\production\\browser\\media\\primeicons-P53SE5CV.woff dist\\production\\browser\\media\\primeicons-DHQU4SEP.svg dist\\production\\browser\\media\\*.ttf dist\\production\\browser\\media\\*.eot -v",
    "del:unused:fonts4": "npx shx --verbose rm -f dist/production/browser/media/primeicons-P53SE5CV.woff dist/production/browser/media/primeicons-DHQU4SEP.svg dist/production/browser/media/*.ttf dist/production/browser/media/*.eot",
    "clean:imports": "ng g @angular/core:cleanup-unused-imports",
    "standalone": "ng generate @angular/core:standalone",
    "ngclass-to-class": "ng generate @angular/core:ngclass-to-class",
    "ngstyle-to-style": "ng generate @angular/core:ngstyle-to-style",
    "to:signal-input": "ng generate @angular/core:signal-input-migration",
    "common-to-standalone": "ng generate @angular/core:common-to-standalone",
    "browserlist:g": "ng generate config browserslist --force",
    "show:stats": "esbuild-visualizer --metadata dist/production/stats.json",
    "build": "npm run write:env -s && ng build",
    "build:fb": "npm run build && npm run copy:board && npm run ngsw:generate:fb",
    "build:stat": "npm run write:env -s && ng build --stats-json && npm run show:stats",
    "build:dev": "npm run write:env -s && ng build --base-href /fosa/ --configuration=dev",
    "build:dev2": "npm run write:env -s && ng build --configuration=dev",
    "build:devMfl": "npm run write:env -s && ng build --configuration=devMfl",
    "mfl:copyconfig": "shx cp src/environments/mfl.config.prod.json dist/production/browser/assets/config.json",
    "fb:copyconfig": "shx cp src/vendors/fb/bf.config.prod.json dist/prodboard/browser/assets/config.json",
    "copy:board": "shx rm -rf dist/prodboard && shx mkdir -p dist/prodboard && shx cp -R dist/production/* dist/prodboard && shx cp -R src/vendors/fb/favicon/* dist/prodboard/browser/assets/img && shx cp  src/vendors/fb/manifest.webmanifest dist/prodboard/browser/manifest.webmanifest && npm run fb:title && npm run fb:meta:desc && npm run fb:copyconfig",
    "fb:title": "shx sed -i \"<title>.*</title>\" \"<title>Fosa Board - MINSANTE</title>\" dist/prodboard/browser/index.html",
    "fb:apptype": "shx sed -i \"\\\"appType\\\": \\\".*\\\"\" \"\\\"appType\\\": \\\"BOARD\\\"\" dist/prodboard/browser/assets/config.json",
    "fb:meta:desc": "shx sed -i \"<meta name=\\\"description\\\" content=\\\".*\\\">\" \"<meta name=\\\"description\\\" content=\\\"Fosa Board - Ministère de la Santé Publique du Cameroun(MINSANTE): Gestion et archivage centralisés des actes réglementaire des formations sanitaires\\\">\" dist/prodboard/browser/index.html",
    "build:test": "npm run write:env -s &@& ng build --configuration=test",
    "build:staging": "npm run write:env -s && ng build --configuration=staging",
    "start": "npm run write:env -s && ng serve --host 0.0.0.0 --proxy-config proxy.conf.js && json-server --watch db.json && concurrently",
    "start:test": "npm run write:env -s && ng serve --host 0.0.0.0 --live-reload false --proxy-config proxy.conf.js && json-server --watch db.json && concurrently",
    "start:win": "npm run write:env -s && ng serve --aot --proxy-config proxy.conf.js",
    "serve:sw": "npm run build -s && npx http-server ./dist -p 4200",
    "lint": "ng lint && stylelint \"src/**/*.scss\" --syntax scss",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:ci": "vitest run --coverage",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean --sort",
    "i18n:init": "ngx-translate-extract --input ./src --output ./src/translations/template.json --key-as-default-value --replace --format json --sort",
    "i18n:extract": "ngx-translate-extract --input ./src --output ./src/assets/i18n/{en,da,de,fi,nb,nl,sv}.json --clean --format json",
    "docs": "hads ./docs -o",
    "write:env": "ngx-scripts env npm_package_version",
    "prettier": "prettier --write \"./src/**/*.{ts,js,html,scss}\"",
    "prettier:check": "prettier --list-different \"./src/**/*.{ts,js,html,scss}\"",
    "postinstallk": "npm run prettier -s && husky install",
    "generate": "ng generate",
    "e2e": "ng e2e",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "dependencies": {
    "@angular/animations": "^21.0.8",
    "@angular/cdk": "^21.0.6",
