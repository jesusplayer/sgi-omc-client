{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/assets/config.json",
          "/polyfills-*.js",
          "/scripts-*.js",
          "/main-*.js",
          "/styles-*.css"
        ]
      }
    },
    {
      "name": "lazy-chunks",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/chunk-*.js"
        ]
      }
    },
    {
      "name": "app-assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**/*.(png|jpg|jpeg|svg|webp|gif|ico|eot|ttf|woff|woff2|otf|cur|ani|mp3|wav|ogg|m4a)",
          "/media/**/*.(png|jpg|jpeg|webp|gif|ico)",
          "/media/primeicons-*.woff2",
          "/media/Flaticon-*.woff",
          "/media/icon-fullscreen-*.svg"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "cdn-data",
      "urls": [
        "https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js",
        "https://cdn.jsdelivr.net/npm/popper.js@1.14.3/dist/umd/popper.min.js",
        "https://cdn.jsdelivr.net/npm/bootstrap@4.1.3/dist/js/bootstrap.min.js",
        "https://cdnjs.cloudflare.com/ajax/libs/jqueryui/1.12.1/jquery-ui.min.js",
        "https://cdnjs.cloudflare.com/ajax/libs/jqueryui-touch-punch/0.2.3/jquery.ui.touch-punch.min.js",
        "https://cdnjs.cloudflare.com/ajax/libs/jquery.scrollbar/0.2.10/jquery.scrollbar.min.js"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 20,
        "maxAge": "30d"
      }
    },
    {
      "name": "api-data",
      "urls": [
        "https://api-fosa.minsante.cm/**"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 200,
        "maxAge": "36h",
        "timeout": "5s"
      }
    }
  ],
  "navigationUrls": [
    "/**",
    "!/fosas/**",
    "!/aires/**",
    "!/communes/**",
    "!/departements/**",
    "!/districts/**",
    "!/regions/**",
    "!/personnels/**",
    "!/arretes/**",
    "!/assets/**",
    "!/**/*.*"
  ]
}